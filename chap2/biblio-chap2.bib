  title = {The {{Use}} of {{Constrained Cellular Automata}} for {{High}}-{{Resolution Modelling}} of {{Urban Land}}-{{Use Dynamics}}},
  author = {White, Roger and Engelen, Guy and Uljee, Inge},
  date = {1997-06-01},
