@inproceedings{yu_abduction_1994,
  langid = {english},
  location = {{New Orleans, Lousiana}},
  title = {Abduction? {{Deduction}}? {{Induction}}? {{Is There}} a {{Logic}} of {{Exploratory Data Analysis}}?},
  url = {https://eric.ed.gov/?id=ED376173},
  abstract = {The philosophical notions introduced by Charles Sanders Peirce (1839-1914) are helpful for researchers in understanding the nature of knowledge and reality. In the Peircean logical system, the logic of abduction and deduction contribute to our conceptual understanding of a phenomenon, while the logic of induction adds quantitative details to the conceptual knowledge. Although Peirce justified the validity of induction as a self-corrective process, he asserted that neither induction nor deduction can help us to unveil the internal structure of meaning. As exploratory data analysis performs the function of a model builder for confirmatory data analysis, abduction plays the role of explorer of viable paths to further inquiry. Thus, the logic of abduction fits well into exploratory  data analysis. At the stage of abduction, the goal is to explore the data, find out a pattern, and suggest a plausible hypothesis; deduction is to refine the hypothesis based upon other plausible premises; and induction is the empirical substantiation. (Contains 55 references.) (Author)},
  eventtitle = {Annual {{Meeting}} of {{American Educational Research Association}}},
  date = {1994-04-04},
  pages = {28},
  keywords = {Comprehension,Deduction,Hypothesis Testing,Induction,Logic},
  author = {Yu, Chong Ho},
  file = {/home/robin/Dropbox/Biblio/Yu/Yu_1994_Abduction.pdf}
}

